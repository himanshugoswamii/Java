package Session16;
import java.applet.*;
import java.awt.*;


public class Smiley extends Applet{
	
	public void paint(Graphics g){
		g.setColor(Color.YELLOW);
		g.fillOval(500,200 , 250, 250);
		g.setColor(Color.BLACK);
		g.fillOval(550, 250, 50, 60);
		g.fillOval(650,250 , 50, 60);
		g.drawArc(560,300 , 125,100 , 180, 180);
	}

	

}
